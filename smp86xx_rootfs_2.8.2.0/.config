#
# Automatically generated make config: don't edit
#
BR2_HAVE_DOT_CONFIG=n
BR2_mipsel=y
BR2_ARCH="mipsel"

#
# Build options
#
BR2_WGET="wget --passive-ftp -nd"
BR2_SVN="Subversion (svn) checkout command"
# BR2_TAR_VERBOSITY is not set
BR2_JLEVEL=1
BR2_DOWNLOAD_SITE="ftp://armutils:sigmadesigns@gw.sigmadesigns.com/smp86xx"

#
# Toolchain Options
#

#
# Common Toolchain Options
#
BR2_TARGET_OPTIMIZATION="-Os -mips32r2 -pipe"

#
# Package Selection for the target
#

#
# The large file support
#
BR2_LARGEFILE=y

#
# The default minimal system
#
BR2_PACKAGE_BUSYBOX=y

#
# Busybox Options
#
BR2_BUSYBOX_VERSION_1_0_0=y
# BR2_BUSYBOX_VERSION_1_5_0 is not set
BR2_BUSYBOX_VERSION="1.00"

#
# The minimum needed to build a uClibc development system
#
BR2_PACKAGE_BASH=y

#
# Other stuff
#
BR2_PACKAGE_CUSTOMIZE=y
BR2_PACKAGE_GDB_SERVER=y
# BR2_PACKAGE_GDB is not set
BR2_PACKAGE_DMALLOC=y
# BR2_PACKAGE_MTD is not set
# BR2_PACKAGE_ZLIB is not set
# BR2_PACKAGE_WIRELESS_TOOLS is not set
BR2_PACKAGE_XENV_UTILS=y
# SD_PACKAGE_TESTAPPS is not set
SD_PACKAGE_BOOT_AUTH=y

#
# Target Options
#
BR2_TARGET_ROOTFS_CRAMFS=y
